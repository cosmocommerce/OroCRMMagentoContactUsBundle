{
    "name": "oro/crm-magento-embedded-contact-us",
    "description": "OroCRM Magento Embedded Contact Us package",
    "homepage": "https://github.com/laboro/OroCRMMagentoContactUsBundle.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "OroCRM\\Bundle\\MagentoContactUsBundle": "src/"
        }
    },
    "target-dir": "src/OroCRM/Bundle/MagentoContactUsBundle",
    "require": {
        "php": ">=5.4.4",
        "oro/platform": "1.0.x-dev",
        "oro/crm": "1.0.x-dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laboro/crm.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/platform.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/OroCRMMagentoContactUsBundle.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
